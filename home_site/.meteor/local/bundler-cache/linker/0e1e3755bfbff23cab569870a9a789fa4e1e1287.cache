                            //\n                    if (pauseOnHover) {                                // 104\n                        el.addEventListener('mouseover', function () {\n                            interval = clearInterval(interval);        // 106\n                        }, false);                                     //\n                        el.addEventListener('mouseout', function () {  // 108\n                            interval = window.setInterval(function () {\n                                that.showCurrent(1); // increment & show\n                            }, speed);                                 //\n                        }, false);                                     //\n                    } // end pauseonhover                              //\n                },                                                     //\n                addFullScreen: function (el) {                         // 116\n                    var that = this,                                   // 117\n                        fsControl = document.createElement(\"span\");    //\n                                                                       //\n                    fsControl.classList.add('bss-fullscreen');         // 120\n                    el.appendChild(fsControl);                         // 121\n                    el.querySelector('.bss-fullscreen').addEventListener('click', function () {\n                        that.toggleFullScreen(el);                     // 123\n                    }, false);                                         //\n                },                                                     //\n                addSwipe: function (el) {                              // 126\n                    var that = this,                                   // 127\n                        ht = new Hammer(el);                           //\n                    ht.on('swiperight', function (e) {                 // 129\n                        that.showCurrent(-1); // decrement & show      // 130\n                    });                                                //\n                    ht.on('swipeleft', function (e) {                  // 132\n                        that.showCurrent(1); // increment & show       // 133\n                    });                                                //\n                },                                                     //\n                toggleFullScreen: function (el) {                      // 136\n                    // https://developer.mozilla.org/en-US/docs/Web/Guide/API/DOM/Using_full_screen_mode\n                    if (!document.fullscreenElement && // alternative standard method\n                    !document.mozFullScreenElement && !document.webkitFullscreenElement && !document.msFullscreenElement) {\n                        // current working methods                     //\n                        if (document.documentElement.requestFullscreen) {\n                            el.requestFullscreen();                    // 142\n                        } else if (document.documentElement.msRequestFullscreen) {\n                            el.msRequestFullscreen();                  // 144\n                        } else if (document.documentElement.mozRequestFullScreen) {\n                            el.mozRequestFullScreen();                 // 146\n                        } else if (document.documentElement.webkitRequestFullscreen) {\n                            el.webkitRequestFullscreen(el.ALLOW_KEYBOARD_INPUT);\n                        }                                              //\n                    } else {                                           //\n                        if (document.exitFullscreen) {                 // 151\n                            document.exitFullscreen();                 // 152\n                        } else if (document.msExitFullscreen) {        //\n                            document.msExitFullscreen();               // 154\n                        } else if (document.mozCancelFullScreen) {     //\n                            document.mozCancelFullScreen();            // 156\n                        } else if (document.webkitExitFullscreen) {    //\n                            document.webkitExitFullscreen();           // 158\n                        }                                              //\n                    }                                                  //\n                } // end toggleFullScreen                              //\n                                                                       //\n            }; // end Slideshow object .....                           //\n                                                                       //\n            // make instances of Slideshow as needed                   //\n            [].forEach.call($slideshows, function (el) {               // 166\n                $slideshow = Object.create(Slideshow);                 // 167\n                $slideshow.init(el, options);                          // 168\n            });                                                        //\n        }                                                              //\n    });                                                                //\n                                                                       //\n    UI.body.events({                                                   // 173\n        'click .clickChangesPage': function (event, template) {        // 174\n            //on event change template                                 //\n            Session.set('page', event.currentTarget.getAttribute('data-page')); //to whatever is in data-page\n        }                                                              //\n    });                                                                //\n}                                                                      //\n                                             