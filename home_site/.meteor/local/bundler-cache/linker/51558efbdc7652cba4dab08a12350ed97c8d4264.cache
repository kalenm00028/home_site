eshow                        //\n    $slideshow = {},                                                   // 3\n        Slideshow = {                                                  //\n        init: function (el, options) {                                 // 5\n            this.counter = 0; // to keep track of current slide        // 6\n            this.el = el; // current slideshow container               // 7\n            this.$items = el.querySelectorAll('figure'); // a collection of all of the slides, caching for performance\n            this.numItems = this.$items.length; // total number of slides\n            options = options || {}; // if options object not passed in, then set to empty object\n            options.auto = options.auto || false; // if options.auto object not passed in, then set to false\n            this.opts = {                                              // 12\n                auto: typeof options.auto === \"undefined\" ? false : options.auto,\n                speed: typeof options.auto.speed === \"undefined\" ? 1500 : options.auto.speed,\n                pauseOnHover: typeof options.auto.pauseOnHover === \"undefined\" ? false : options.auto.pauseOnHover,\n                fullScreen: typeof options.fullScreen === \"undefined\" ? false : options.fullScreen,\n                swipe: typeof options.swipe === \"undefined\" ? false : options.swipe\n            };                                                         //\n                                                                       //\n            this.$items[0].classList.add('bss-show'); // add show class to first figure\n            this.injectControls(el);                                   // 21\n            this.addEventListeners(el);                                // 22\n            if (this.opts.auto) {                                      // 23\n                this.autoCycle(this.el, this.opts.speed, this.opts.pauseOnHover);\n            }                                                          //\n            if (this.opts.fullScreen) {                                // 26\n                this.addFullScreen(this.el);                           // 27\n            }                                                          //\n            if (this.opts.swipe) {                                     // 29\n                this.addSwipe(this.el);                                // 30\n            }                                                          //\n        },                                                             //\n        showCurrent: function (i) {                                    // 33\n            // increment or decrement this.counter depending on whether i === 1 or i === -1\n            if (i > 0) {                                               // 35\n                this.counter = this.counter + 1 === this.numItems ? 0 : this.counter + 1;\n            } else {                                                   //\n                this.counter = this.counter - 1 < 0 ? this.numItems - 1 : this.counter - 1;\n            }                                                          //\n                                                                       //\n            // remove .show from whichever element currently has it    //\n            // http://stackoverflow.com/a/16053538/2006057             //\n            [].forEach.call(this.$items, function (el) {               // 43\n                el.classList.remove('bss-show');                       // 44\n            });                                                        //\n                                                                       //\n            // add .show to the one item that's supposed to have it    //\n            this.$items[this.counter].classList.add('bss-show');       // 48\n        },                                                             //\n        injectControls: function (el) {                                // 50\n            // build and inject prev/next controls                     //\n            // first create all the new elements                       //\n            var spanPrev = document.createElement(\"span\"),             // 53\n                spanNext = document.createElement(\"span\"),             //\n                docFrag = document.createDocumentFragment();           //\n                                                                       //\n            // add classes                                             //\n            spanPrev.classList.add('bss-prev');                        // 58\n            spanNext.classList.add('bss-next');                        // 59\n                                                                       //\n            // add contents                                            //\n            spanPrev.innerHTML = '&laquo;';                            // 62\n            spanNext.innerHTML = '&raquo;';                            // 63\n                                                                       //\n            // append elements to fragment, then append fragment to DOM\n            docFrag.appendChild(spanPrev);                             // 66\n            docFrag.appendChild(spanNext);                             // 67\n            el.appendChild(docFrag);                                   // 68\n        },                                                             //\n        addEventListeners: function (el) {                             // 70\n            var that = this;                                           // 71\n            el.querySelector('.bss-next').addEventListener('click', function () {\n                that.showCurrent(1); // increment & show               // 73\n            }, false);                                                 //\n                                                                       //\n            el.querySelector('.bss-prev').addEventListener('click', function () {\n                that.showCurrent(-1); // decrement & show              // 77\n            }, false);                                                 //\n                                                                       //\n            el.onkeydown = function (e) {                              // 80\n                e = e || window.event;                                 // 81\n                if (e.keyCode === 37) {                                // 82\n                    that.showCurrent(-1); // decrement & show          // 83\n                } else if (e.keyCode === 39) {                         //\n                        that.showCurrent(1); // increment & show       // 85\n                    }                                                  //\n            };                                                         //\n        },                                                             //\n        autoCycle: function (el, speed, pauseOnHover) {                // 89\n            var that = this,                                           // 90\n                interval = window.setInterval(function () {            //\n                that.showCurrent(1); // increment & show               // 92\n            }, speed);                                                 //\n                                                                       //\n            if (pauseOnHover) {                                        // 95\n                el.addEventListener('mouseover', function () {         // 96\n                    interval = clearInterval(interval);                // 97\n                }, false);                                             //\n                el.addEventListener('mouseout', function () {          // 99\n                    interval = window.setInterval(function () {        // 100\n                        that.showCurrent(1); // increment & show       // 101\n                    }, speed);                                         //\n                }, false);                                             //\n            } // end pauseonhover                                      //\n        },                                                             //\n        addFullScreen: function (el) {                                 // 107\n            var that = this,                                           // 108\n                fsControl = document.createElement(\"span\");            //\n                                                                       //\n            fsControl.classList.add('bss-fullscreen');                 // 111\n            el.appendChild(fsControl);                                 // 112\n            el.querySelector('.bss-fullscreen').addEventListener('click', function () {\n                that.toggleFullScreen(el);                             // 114\n            }, false);                                                 //\n        },                                                             //\n        addSwipe: function (el) {                                      // 117\n            var that = this,                                           // 118\n                ht = new Hammer(el);                                   //\n            ht.on('swiperight', function (e) {                         // 120\n                that.showCurrent(-1); // decrement & show              // 121\n            });                                                        //\n            ht.on('swipeleft', function (e) {                          // 123\n                that.showCurrent(1); // increment & show               // 124\n            });                                                        //\n        },                                                             //\n        toggleFullScreen: function (el) {                              // 127\n            // https://developer.mozilla.org/en-US/docs/Web/Guide/API/DOM/Using_full_screen_mode\n            if (!document.fullscreenElement && // alternative standard method\n            !document.mozFullScreenElement && !document.webkitFullscreenElement && !document.msFullscreenElement) {\n                // current working methods                             //\n                if (document.documentElement.requestFullscreen) {      // 132\n                    el.requestFullscreen();                            // 133\n                } else if (document.documentElement.msRequestFullscreen) {\n                    el.msRequestFullscreen();                          // 135\n                } else if (document.documentElement.mozRequestFullScreen) {\n                    el.mozRequestFullScreen();                         // 137\n                } else if (document.documentElement.webkitRequestFullscreen) {\n                    el.webkitRequestFullscreen(el.ALLOW_KEYBOARD_INPUT);\n                }                                                      //\n            } else {                                                   //\n                if (document.exitFullscreen) {                         // 142\n                    document.exitFullscreen();                         // 143\n                } else if (document.msExitFullscreen) {                //\n                    document.msExitFullscreen();                       // 145\n                } else if (document.mozCancelFullScreen) {             //\n                    document.mozCancelFullScreen();                    // 147\n                } else if (document.webkitExitFullscreen) {            //\n                    document.webkitExitFullscreen();                   // 149\n                }                                                      //\n            }                                                          //\n        } // end toggleFullScreen                                      //\n                                                                       //\n    }; // end Slideshow object .....                                   //\n                                                                       //\n    // make instances of Slideshow as needed                           //\n    [].forEach.call($slideshows, function (el) {                       // 157\n        $slideshow = Object.create(Slideshow);                         // 158\n        $slideshow.init(el, options);                                  // 159\n    });                                                                //\n};                                                                     //\n/////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n","servePath":"/slideshow.js","sourceMap":{"version":3,"sources":["meteor://ğŸ’»app/slideshow.js"],"names":[],"mappings":";;;;;;;;AAAA,IAAI,OAAO,GAAG,UAAU,EAAE,EAAE,OAAO,EAAE;AACjC,QAAI,WAAW,GAAG,QAAQ,CAAC,gBAAgB,CAAC,EAAE,CAAC;;AAC3C,cAAU,GAAG,EAAE;QACf,SAAS,GAAG;AACR,YAAI,EAAE,UAAU,EAAE,EAAE,OAAO,EAAE;AACzB,gBAAI,CAAC,OAAO,GAAG,CAAC,CAAC;AACjB,gBAAI,CAAC,EAAE,GAAG,EAAE,CAAC;AACb,gBAAI,CAAC,MAAM,GAAG,EAAE,CAAC,gBAAgB,CAAC,QAAQ,CAAC,CAAC;AAC5C,gBAAI,CAAC,QAAQ,GAAG,IAAI,CAAC,MAAM,CAAC,MAAM,CAAC;AACnC,mBAAO,GAAG,OAAO,IAAI,EAAE,CAAC;AACxB,mBAAO,CAAC,IAAI,GAAG,OAAO,CAAC,IAAI,IAAI,KAAK,CAAC;AACrC,gBAAI,CAAC,IAAI,GAAG;AACR,oBAAI,EAAG,OAAO,OAAO,CAAC,IAAI,KAAK,WAAW,GAAI,KAAK,GAAG,OAAO,CAAC,IAAI;AAClE,qBAAK,EAAG,OAAO,OAAO,CAAC,IAAI,CAAC,KAAK,KAAK,WAAW,GAAI,IAAI,GAAG,OAAO,CAAC,IAAI,CAAC,KAAK;AAC9E,4BAAY,EAAG,OAAO,OAAO,CAAC,IAAI,CAAC,YAAY,KAAK,WAAW,GAAI,KAAK,GAAG,OAAO,CAAC,IAAI,CAAC,YAAY;AACpG,0BAAU,EAAG,OAAO,OAAO,CAAC,UAAU,KAAK,WAAW,GAAI,KAAK,GAAG,OAAO,CAAC,UAAU;AACpF,qBAAK,EAAG,OAAO,OAAO,CAAC,KAAK,KAAK,WAAW,GAAI,KAAK,GAAG,OAAO,CAAC,KAAK;aACxE,CAAC;;AAEF,gBAAI,CAAC,MAAM,CAAC,CAAC,CAAC,CAAC,SAAS,CAAC,GAAG,CAAC,UAAU,CAAC,CAAC;AACzC,gBAAI,CAAC,cAAc,CAAC,EAAE,CAAC,CAAC;AACxB,gBAAI,CAAC,iBAAiB,CAAC,EAAE,CAAC,CAAC;AAC3B,gBAAI,IAAI,CAAC,IAAI,CAAC,IAAI,EAAE;AAChB,oBAAI,CAAC,SAAS,CAAC,IAAI,CAAC,EAAE,EAAE,IAAI,CAAC,IAAI,CAAC,KAAK,EAAE,IAAI,CAAC,IAAI,CAAC,YAAY,CAAC,CAAC;aACpE;AACD,gBAAI,IAAI,