nt.mozRequestFullScreen) {\n                            el.mozRequestFullScreen();                 // 154\n                        } else if (document.documentElement.webkitRequestFullscreen) {\n                            el.webkitRequestFullscreen(el.ALLOW_KEYBOARD_INPUT);\n                        }                                              //\n                    } else {                                           //\n                        if (document.exitFullscreen) {                 // 159\n                            document.exitFullscreen();                 // 160\n                        } else if (document.msExitFullscreen) {        //\n                            document.msExitFullscreen();               // 162\n                        } else if (document.mozCancelFullScreen) {     //\n                            document.mozCancelFullScreen();            // 164\n                        } else if (document.webkitExitFullscreen) {    //\n                            document.webkitExitFullscreen();           // 166\n                        }                                              //\n                    }                                                  //\n                } // end toggleFullScreen                              //\n                                                                       //\n            }; // end Slideshow object .....                           //\n                                                                       //\n            // make instances of Slideshow as needed                   //\n            [].forEach.call($slideshows, function (el) {               // 174\n                $slideshow = Object.create(Slideshow);                 // 175\n                $slideshow.init(el, options);                          // 176\n            });                                                        //\n        }                                                              //\n    });                                  